{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": "ts",
    "outDir": "build",
    "target": "es2017",
    "lib": [
      "es2017"
    ],
    "baseUrl": ".",
    "paths": {
      "auto-push": ["node_modules/auto-push/build/src/index"]
    }
  },
  "include": [
    "ts/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
